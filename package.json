    "./dist/event-iterator": "./dist/event-iterator-browser",
    "./dist/fetch": "cross-fetch",
    "fetch-h2": false,
    "path": false,
    "resolve-alpn": false,
    "ws": false
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "mocha",
    "build": "tsc -b",
    "dev": "tsc -w",
    "prettier": "prettier --write .",
    "prepare": "npm run build",
    "prestart": "npm run build",
    "pretest": "npm run build"
  },
  "mocha": {
    "require": [
      "ts-node/register"
    ],
    "extension": "test.ts",
    "exit": true,
    "timeout": 10000
