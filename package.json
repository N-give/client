  "main": "dist/index.js",
  "engines": {
    "node": ">=12.16.0"
  },
  "browser": {
    "./dist/event-iterator": "./dist/event-iterator-browser",
    "./dist/fetch": "cross-fetch",
    "fetch-h2": false,
    "path": false,
    "resolve-alpn": false,
    "ws": false
  },
